public class age {
	 public static void main(String[] args) {
		 int age;
		 int a;
		 String b;
		 String name="sathya";
		 Scanner in = new Scanner(System.in);  
	     System.out.print("Enter the birthyear: ");  
	     a= in.nextInt();
