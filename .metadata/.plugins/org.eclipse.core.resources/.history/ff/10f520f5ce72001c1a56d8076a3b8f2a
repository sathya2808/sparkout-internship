package get10i;

import java.util.Scanner;

public class Uppercase {
	public static void main(String [] args)  {
		 int i;
		 Scanner in = new Scanner(System.in);  
	     System.out.print("Enter i value: ");  
	     i = in.nextInt(); 
	     String[] array = {}; 
	     for(int j=0;j<i;j++) { 
	    	 Scanner ij = new Scanner(System.in)
		     array[j] = ij.nextLine(); }
	     }
	     
	     }
	     
	

