package pattern;

import java.util.Scanner;

public class patterns {
	public static void main(String[] args) {
	Scanner scan =new Scanner(System.in);
	System.out.println("enter the limit:");
	int i=scan.nextInt();
	for(int l = 5 ; l <=1; --l)
	{
		for (int j=1;j<=l;++j)
		{
			System.out.print("*");
		}
		System.out.println();
	}	
}
}