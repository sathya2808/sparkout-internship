package get10i;

import java.util.Scanner;

public class Uppercase {
	public static void main(String [] args)  {
		 int i;
		 Scanner in = new Scanner(System.in);  
	     System.out.print("Enter i value: ");  
	     i = in.nextInt(); 
	     int[] array = new int[i]; 
	     for(int j=0;j<i;j++) { 
		     array[j] = in.nextLine(); }
	     
	     }
	     
	     }
	     
	

