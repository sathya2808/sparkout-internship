package primenumber;

public class PN {
public static void main(String[] args) {
	for(int i =2;i<100;i++) {
		for(int j=2 ; j<i/2;j++)
	if(i==2 || i==3) {
		System.out.println("prime"+i);
	}
     else if (i%2==0 || i%3==0 ) {
		System.out.println("not prime"+i);
		
	}
	else {
		System.out.println("prime"+i);
	}
	}}}